{
    "manifest_version": 3,
    "name": "URL Shortener",
    "version": "1.0",
    "description": "One-click URL shortener extension",
    "icons": {
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png"
    },
    "permissions": ["activeTab", "storage", "clipboardWrite"],
    "action": {
      "default_icon": "icons/icon-48.png",
      "default_popup": "popup.html"
    }
  }

